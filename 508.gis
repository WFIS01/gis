async function run(input) {
  const id = input.id;

  try {
    const res = await fetch(
      "https://assetdelivery.roblox.com/v1/asset/?id=" + id,
      {
        method: "GET",
        headers: {
          "Range": "bytes=0-0"
        }
      }
    );

    const type = res.headers.get("content-type") || "";

    return {
      id,
      valid: res.ok && type.startsWith("audio/")
    };
  } catch (e) {
    return {
      id,
      valid: false,
      error: "fetch_failed"
    };
  }
}
